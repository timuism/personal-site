---
export interface Props {
	href: string;
}

const { href } = Astro.props;
---
<a href={href} class="inline-block whitespace-nowrap text-gray-100 hover:text-secondary focus:text-secondary outline-none border-b border-gray-500 font-medium text-base hover:border-gray-100 duration-200">
  <slot />
</a>
